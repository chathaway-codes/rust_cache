<h1>Listing caches</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Server</th>
      <th>Name</th>
      <th>X</th>
      <th>Y</th>
      <th>Z</th>
      <th>Secured</th>
      <th>Notes</th>
      <th></th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @caches.each do |cach| %>
      <tr>
        <td><%= cach.server %></td>
        <td><%= cach.name %></td>
        <td><%= cach.x %></td>
        <td><%= cach.y %></td>
        <td><%= cach.z %></td>
        <td><%= cach.secured %></td>
        <td><%= cach.notes %></td>
        <td><%= link_to 'Show', cach %></td>
        <td><%= link_to 'Share', cach_share_path(cach) %></td>
        <td><%= link_to 'Edit', edit_cach_path(cach) %></td>
        <td><%= link_to 'Destroy', cach, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cache', new_cach_path %>
